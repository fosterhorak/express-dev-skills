<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>Express Developer Skills</title>
</head>
<body>
    <a href="/">Home</a>
    <h1>Devloper Skills List</h1>
    <ul>
        <% skills.forEach(function(s) { %>
        <li>
            <a href="/skills/<%= s.skill %>"><%= s.skill %></a>
            - 
            <%= s.has ? "have" : "don't have" %>
        </li>
        <% }); %>
    </ul>
    <a href="/skills/new">Add a new skill</a>
</body>
</html>